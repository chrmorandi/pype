{
  "name": "jacmoe/pype",
  "description": "Personal Yii Page Engine",
  "keywords": ["yii2", "framework", "basic", "sass", "gulp", "project template"],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.5",
    "jacmoe/yii2-mdpages-module": "@dev",
    "jamesmoss/flywheel": "^0.4.2",
    "scrivo/highlight.php": "^9.0",
    "jacmoe/yii2-tale-jade": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
},
      "repositories": [
          {
              "type": "vcs",
              "url": "https://github.com/jacmoe/yii2-mdpages-module"
          }
      ],
  "config": {
    "process-timeout": 1800
  },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
